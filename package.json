{
  "name": "isomorphic-demo",
  "version": "0.0.1",
  "description": "A boilerplate for isomorphic React apps",
  "main": "lib/server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf lib",
    "watch-js": "./node_modules/.bin/babel src -d lib --experimental -w",
    "dev-server": "node lib/server/webpack",
    "server": "nodemon lib/server/server",
    "start": "npm run watch-js & npm run dev-server & npm run server",
    "build": "npm run clean && ./node_modules/.bin/babel src -d lib --experimental",
    "deploy": "NODE_ENV=production webpack -p --config webpack.config.dev.js"
  },
  "keywords": [
    "isomorphic",
    "react",
    "app"
  ],
  "author": "Joseph Furlott",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.5.6",
    "babel-core": "^5.5.6",
    "babel-loader": "^5.1.4",
    "css-loader": "^0.19.0",
    "express": "^4.12.4",
    "extract-text-webpack-plugin": "^0.8.2",
    "hjs": "~0.0.6",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.3.3",
    "react": "^0.13.3",
    "react-hot-loader": "^1.2.7",
    "react-router": "^0.13.3",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "nodemon": "^1.3.7",
    "react-component-visibility": "0.0.8"
  }
}
